{
    "type":"document",
    "props":{

    },
    "children":[
        {
            "type":"section",
            "props":{
                "pgSz":{
					"width":800,
					"height":600
				},
				"pgMar":{
					"left":20,
					"right":20,
					"top":20,
					"bottom":20,
					"header":15,
					"footer":15
				}
            },
            "children":[
                {
                    "type":"paragraph",
                    "props":{
						"defaultStyle":{
							"fonts":"arial",
                            "size":12
						}
                    },
                    "children":[
                        {
                            "type":"text",
                            "props":{
                                "fonts":"arial",
                                "size":12,
                                "color":"black"
                            },
                            "children":"hello raymond Video provides a powerful way to help you prove your point. When you click Online Video, you can paste in the embed code for the video you want to add. "
                        }
                    ]
                },
                {
                    "type":"paragraph",
                    "props":{
						"defaultStyle":{
							"fonts":"arial",
                            "size":12
						}
                    },
                    "children":[
                        {
                            "type":"$for",
                            "props":{
                                "init":"var i=children.length-1",
                                "test":"i>=0",
                                "update":"i--"
                            },
                            "children":[
                                {
                                    "type":"$exp",
                                    "props":{
                                        "expression":"children[i].name+','"
                                    },
                                    "children":[
                                        {
                                            "type":"text",
                                            "props":{
                                                "fonts":"calibri",
                                                "size":20,
                                                "color":"red"
                                            },
											"children":""
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },

                {
                    "type":"paragraph",
                    "props":{
						"defaultStyle":{
							"fonts":"arial",
                            "size":12
						}
                    },
                    "children":[
                        {
                            "type":"$for",
                            "props":{
                                "init":"var i=0",
                                "test":"i<4",
                                "update":"i++"
                            },
                            "children":[
                                {
                                    "type":"text",
                                    "props":{
                                        "fonts":"arial",
                                        "size":12,
                                        "color":"blue"
                                    },
                                    "children":"hello "
                                },
                                {
                                    "type":"$exp",
                                    "props":{
                                        "expression":"`${lastName} ${i},`",
                                    },
                                    "children":[
                                        {
                                            "type":"text",
                                            "props":{
                                                "fonts":"arial",
                                                "size":12,
                                                "color":"blue"
                                            },
                                            "children":""
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "type":"$for",
                    "props":{
                        "init":"var i=0",
                        "test":"i<5",
                        "update":"i++"
                    },
                    "children":[
                        {
                            "type":"paragraph",
                            "props":{
								"defaultStyle":{
									"fonts":"arial",
									"size":12
								}
                            },
                            "children":[
                                {
                                    "type":"$for",
                                    "props":{
                                        "init":"var i=0",
                                        "test":"i<4",
                                        "update":"i++"
                                    },
                                    "children":[
                                        {
                                            "type":"text",
                                            "props":{
                                                "fonts":"arial",
                                                "size":12,
                                                "color":"blue"
                                            },
                                            "children":"hello "
                                        },
                                        {
                                            "type":"$exp",
                                            "props":{
                                                "expression":"`${lastName} ${i},`"
                                            },
                                            "children":[
                                                {
                                                    "type":"text",
                                                    "props":{
                                                        "fonts":"arial",
                                                        "size":12,
                                                        "color":"blue"
                                                    },
                                                    "children":""
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "type":"$exp",
                                    "props":{
                                        "expression":"`----${i+''}`"
                                    },
                                    "children":[
                                        {
                                            "type":"text",
                                            "props":{
                                                "fonts":"arial",
                                                "size":12,
                                                "color":"blue",
                                                "bold":true,
                                            },
                                            "children":""
                                        }
                                    ]
                                },
                            ]
                        }
                    ]
                },

                {
                    "type":"paragraph",
                    "props":{
						"defaultStyle":{
							"fonts":"arial",
                            "size":12
						}
                    },
                    "children":[
                        {
                            "type":"$if",
                            "props":{
                                "condition":"isEmployee"
                            },
                            "children":[
                                {
                                    "type":"text",
                                    "props":{
                                        "fonts":"arial",
                                        "size":12,
                                        "color":"red"
                                    },
                                    "children":"hello raymond Video provides a powerful way to help you prove your point. When you click Online Video, you can paste in the embed code for the video you want to add. "
                                },
                                {
                                    "type":"text",
                                    "props":{
                                        "fonts":"arial",
                                        "size":12,
                                        "color":"yellow"
                                    },
                                    "children":"hello raymond Video provides a powerful way to help you prove your point. When you click Online Video, you can paste in the embed code for the video you want to add. "
                                }
                            ]
                        }
                        ,
                        {
                            "type":"$exp",
                            "props":{
                                "expression":"lastName+' '+firstName",
                                "name":"name",
                            },
                            "children":[
                                {
                                    "type":"text",
                                    "props":{
                                        "fonts":"arial",
                                        "size":12,
                                        "color":"red"
                                    },
                                    "children":""
                                }
                            ]
                        }
                    ]
                }
 
			]
        }
    ]
}
